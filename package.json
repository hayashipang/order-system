{
  "name": "backend_v3_ready",
  "version": "3.0.0",
  "main": "server_v3.js",
  "type": "module",
  "scripts": {
    "start": "node server_v4.js",
    "start-json": "node server_v3.js",
    "start-sqlite": "node server_v4.js",
    "dev": "nodemon server_v4.js",
    "dev-json": "nodemon server_v3.js",
    "dev-sqlite": "nodemon server_v4.js",
    "clear-data": "node clear-all-data.js",
    "init-db": "node init-database.js",
    "init-sqlite": "node init-sqlite-db.js",
    "migrate": "node migrate-data.js",
    "migrate-sqlite": "node migrate-to-sqlite.js",
    "build": "cd client && npm run build",
    "postinstall": "cd client && npm install",
    "verify:all": "bash verify/scripts/test-all-apis.sh",
    "verify:smoke": "bash verify/scripts/smoke.sh",
    "verify:scan": "node verify/scripts/scan-api.js",
    "test:verify": "jest --runInBand --roots verify/__tests__",
    "e2e:verify": "playwright test verify/e2e",
    "scan:props": "node scripts/scan-unused-props.js",
    "fix:props": "node scripts/fix-unused-props.js"
  },
  "dependencies": {
    "async": "^3.2.6",
    "axios": "^1.13.0",
    "body-parser": "^1.20.2",
    "colors": "^1.4.0",
    "commander": "^13.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "pg": "^8.11.3",
    "sqlite3": "^5.1.7",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "express-list-endpoints": "^7.1.1",
    "jest": "^29.7.0",
    "jq": "^1.7.2",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.4"
  }
}
